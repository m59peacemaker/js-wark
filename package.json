    "behavior",
    "dynamic",
    "emitter",
    "observable",
    "stream",
    "computed",
    "state"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/m59peacemaker/js-wark"
  },
  "author": "Johnny Hauser <johnnyhauser@gmail.com>",
  "license": "CC0-1.0",
  "dependencies": {},
  "devDependencies": {
    "delay": "^4.3.0",
    "glob-module-file": "^2.2.1",
    "onchange": "^6.1.0",
    "reify": "^0.18.1",
    "tape": "^4.9.1",
    "zora": "^3.1.8"
  }
}
